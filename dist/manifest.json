{
  "manifest_version": 3,
  "name": "Chrome Extension Boilerplate",
  "description": "Javascript + React + TailwindCSS",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["js/content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "128": "128.png"
  }
}
